<configuration>
	<system.runtime.remoting>
		<customErrors mode="off"/>
	</system.runtime.remoting>
	<system.diagnostics>
		<switches>
			<add name="AssertSwitch" value="1"/>
			<add name="LogSwitch" value="4"/>
		</switches>
	</system.diagnostics>
	<appSettings>
		<add key="DefaultDirectory" 		value=""/> 			<!-- string -->
		<add key="P" 						value="11"/> 		<!-- int -->
		<add key="ProxyAddress" 			value=""/> 			<!-- string -->
		<add key="ProxyBypassList" 			value=""/> 			<!-- string --> <!-- Separate with semicolons -->
		<add key="NoSound" 					value="false"/> 	<!-- bool -->
		<add key="MultiplayerBasePort" 		value="20172"/> 	<!-- int --> <!-- Define a contiguous set of multiplayer ports that are available -->
		<add key="MultiplayerPortCount" 	value="10"/> 		<!-- int -->
		<add key="ClientDrawStepPeriod" 	value="250"/> 		<!-- int -->
		<add key="PasswordsForMultiplayer" 	value="false"/> 	<!-- bool -->
		<add key="SupportedLanguageCodes" 	value=""/> 			<!-- string -->
		<add key="SupportedLanguageNames" 	value=""/> 			<!-- string -->
		<add key="PreferredLanguageCode" 	value=""/> 			<!-- string -->
		<add key="ScoreAdapterServerName" 	value=""/> 			<!-- string -->
	</appSettings>
</configuration>
